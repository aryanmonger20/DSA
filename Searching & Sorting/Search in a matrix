	int matSearch (vector <vector <int>> &arr, int N, int M, int x)
	{
	    // your code here
	    int i=0,j=M-1;
	    
	    while(i>=0 && i<N && j>=0 &&j<M){
	        if(arr[i][j]==x)
	        return 1;
	        else
	        if(arr[i][j]>x)
	        j--;
	        else
	        i++;
	    }
	    return 0;
	}
};