/////////Inorder/////////
class Solution {
public:
    vector<int> inOrder(Node* root)
    {
        //code here
        vector<int>ans;
        stack<Node*>s;
        
        while(root!=NULL || s.empty()==false){
           while(root!=NULL)
           { 
              s.push(root);
             root=root->left;
            
        }
      root=s.top();
        s.pop();
        ans.push_back(root->data);
        root=root->right;
        }
        return ans;
       
    }
////////Preorder///////
vector<int> preOrder(Node* root)
{
    //code here
     vector<int>ans;
        stack<Node*>s;
         s.push(root);
        
        while(s.empty()==false){
      
     
           while(root!=NULL)
           
           {  
         
          ans.push_back(root->data);
             s.push(root);
             root=root->left;
            
        }
        root=s.top();
          s.pop();
        root=root->right;
        }
        return ans;
}